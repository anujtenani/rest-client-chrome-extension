{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "externally_connectable": {
    "matches": ["*://cbco.in/*"]
  },
  "permissions": ["storage","unlimitedStorage","<all_urls>","cookies","webRequest"],
  "background": {
    "scripts": ["background.js"]
  }
}
